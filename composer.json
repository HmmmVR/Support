{
    "name": "hmmmvr/support",
    "type": "library",
    "description": "PHP helpers",
    "keywords": [
        "HmmmVR",
        "Support"
    ],
    "homepage": "https://github.com/HmmmVR/Support",
    "license": "MIT",
    "authors": [
        {
            "name": "HmmmVR",
            "email": "dev.kobus@gmail.com",
            "homepage": "https://github.com/HmmmVR",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "HmmmVR\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HmmmVR\\Support\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
